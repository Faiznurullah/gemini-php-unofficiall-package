{
    "name": "faiznurullah/gemini-php-unofficial",
    "description": "package for integrated gemini AI with php projects",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Faiznurullah\\Gemini\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Faiz Nurullah"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "guzzlehttp/guzzle": "^7.8", 
        "illuminate/support": "^6|^7|^8|^9|^10|^11",
        "phpoption/phpoption": "^1.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5" 
    },
    "extra": {
        "laravel": {
          "providers": [
            "Faiznurullah\\Gemini\\GeminiServiceProvider"
          ],
          "aliases": {
            "Gemini": "Faiznurullah\\Gemini\\GeminiFacade"
          }
        }
      }
}
